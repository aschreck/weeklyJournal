{
  "name": "weekly-journal-backend",
  "version": "1.0.0",
  "description": "a backend for the weekly journal application",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "npm run start --prefix client",
    "server": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
